{
    "type": "commonjs",
    "scripts": {
        "dev": "dotenv -e .env.local -- concurrently \"ts-node-dev --max-old-space-size=4096 --respawn --transpile-only --project backend/tsconfig.json backend/src/index.ts\" \"vite\"",
        "dev:frontend": "dotenv -e .env.local -- vite",
        "dev:server": "dotenv -e .env.local -- ts-node-dev --respawn --transpile-only --project backend/tsconfig.json backend/src/index.ts",
        "dev:prod": "dotenv -e .env.production -- ts-node-dev --respawn --transpile-only --project backend/tsconfig.json backend/src/index.ts",
        "build": "ts-node scripts/generateViteEnv.ts && tsc && vite build",
        "preview": "vite preview",
        "start": "dotenv -e .env.production -- ts-node src/index.ts",
        "build:server": "tsc -p backend/tsconfig.json",
        "db:init": "dotenv -e .env.local -- ts-node backend/src/scripts/initDb.ts",
        "db:init:prod": "dotenv -e .env.production -- ts-node backend/src/scripts/initDb.ts",
        "deploy": "anchor deploy --provider.cluster localnet",
        "test": "anchor test --provider.cluster localnet",
        "calculate-market-caps": "tsc scripts/calculateMissingMarketCaps.ts && node scripts/calculateMissingMarketCaps.js",
        "dev:backend-test": "ts-node-dev --respawn --transpile-only backend/src/index.ts",
        "test:aws": "ts-node scripts/testAwsAccess.ts",
        "test:aws:local": "ts-node scripts/testAwsAccess.ts local",
        "test:aws:prod": "ts-node scripts/testAwsAccess.ts prod",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
